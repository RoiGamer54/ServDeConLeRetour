{"ast":null,"code":"import { Pie } from 'vue-chartjs/legacy';\nimport { Chart as ChartJS, Title, Tooltip, Legend, ArcElement, CategoryScale } from 'chart.js';\nChartJS.register(Title, Tooltip, Legend, ArcElement, CategoryScale);\nexport default {\n  props: {\n    diskUsage: {\n      type: Object,\n      required: true\n    }\n  },\n  extends: Pie,\n  mounted() {\n    const labels = Object.keys(this.diskUsage);\n    const usedSpace = Object.values(this.diskUsage).map(disk => disk.used);\n    const freeSpace = Object.values(this.diskUsage).map(disk => disk.free);\n    this.renderChart({\n      labels: labels,\n      datasets: [{\n        label: 'Utilisé (Go)',\n        backgroundColor: ['#FF6384', '#36A2EB', '#FFCE56', '#8BC34A'],\n        data: usedSpace\n      }, {\n        label: 'Libre (Go)',\n        backgroundColor: ['#FFCDD2', '#BBDEFB', '#FFF59D', '#DCEDC8'],\n        data: freeSpace\n      }]\n    }, {\n      responsive: true,\n      maintainAspectRatio: false\n    });\n  }\n};","map":{"version":3,"names":["Pie","Chart","ChartJS","Title","Tooltip","Legend","ArcElement","CategoryScale","register","props","diskUsage","type","Object","required","extends","mounted","labels","keys","usedSpace","values","map","disk","used","freeSpace","free","renderChart","datasets","label","backgroundColor","data","responsive","maintainAspectRatio"],"sources":["C:\\Users\\GUERBER\\Desktop\\SERVDECON\\PROJET\\FRONTEND\\frontend-vvcraft2\\src\\components\\Serveurs\\DiskPieChart.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h3>Utilisation des Disques</h3>\r\n      <canvas id=\"disk-usage-chart\"></canvas>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { Pie } from 'vue-chartjs/legacy';\r\n  import { Chart as ChartJS, Title, Tooltip, Legend, ArcElement, CategoryScale } from 'chart.js';\r\n  \r\n  ChartJS.register(Title, Tooltip, Legend, ArcElement, CategoryScale);\r\n  \r\n  export default {\r\n    props: {\r\n      diskUsage: {\r\n        type: Object,\r\n        required: true\r\n      }\r\n    },\r\n    extends: Pie,\r\n    mounted() {\r\n      const labels = Object.keys(this.diskUsage);\r\n      const usedSpace = Object.values(this.diskUsage).map(disk => disk.used);\r\n      const freeSpace = Object.values(this.diskUsage).map(disk => disk.free);\r\n  \r\n      this.renderChart({\r\n        labels: labels,\r\n        datasets: [\r\n          {\r\n            label: 'Utilisé (Go)',\r\n            backgroundColor: ['#FF6384', '#36A2EB', '#FFCE56', '#8BC34A'],\r\n            data: usedSpace\r\n          },\r\n          {\r\n            label: 'Libre (Go)',\r\n            backgroundColor: ['#FFCDD2', '#BBDEFB', '#FFF59D', '#DCEDC8'],\r\n            data: freeSpace\r\n          }\r\n        ]\r\n      }, {\r\n        responsive: true,\r\n        maintainAspectRatio: false\r\n      });\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  canvas {\r\n    max-width: 500px;\r\n    margin: auto;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAQE,SAASA,GAAE,QAAS,oBAAoB;AACxC,SAASC,KAAI,IAAKC,OAAO,EAAEC,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,UAAU,EAAEC,aAAY,QAAS,UAAU;AAE9FL,OAAO,CAACM,QAAQ,CAACL,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,UAAU,EAAEC,aAAa,CAAC;AAEnE,eAAe;EACbE,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAO,EAAEd,GAAG;EACZe,OAAOA,CAAA,EAAG;IACR,MAAMC,MAAK,GAAIJ,MAAM,CAACK,IAAI,CAAC,IAAI,CAACP,SAAS,CAAC;IAC1C,MAAMQ,SAAQ,GAAIN,MAAM,CAACO,MAAM,CAAC,IAAI,CAACT,SAAS,CAAC,CAACU,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACC,IAAI,CAAC;IACtE,MAAMC,SAAQ,GAAIX,MAAM,CAACO,MAAM,CAAC,IAAI,CAACT,SAAS,CAAC,CAACU,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACG,IAAI,CAAC;IAEtE,IAAI,CAACC,WAAW,CAAC;MACfT,MAAM,EAAEA,MAAM;MACdU,QAAQ,EAAE,CACR;QACEC,KAAK,EAAE,cAAc;QACrBC,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;QAC7DC,IAAI,EAAEX;MACR,CAAC,EACD;QACES,KAAK,EAAE,YAAY;QACnBC,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;QAC7DC,IAAI,EAAEN;MACR;IAEJ,CAAC,EAAE;MACDO,UAAU,EAAE,IAAI;MAChBC,mBAAmB,EAAE;IACvB,CAAC,CAA