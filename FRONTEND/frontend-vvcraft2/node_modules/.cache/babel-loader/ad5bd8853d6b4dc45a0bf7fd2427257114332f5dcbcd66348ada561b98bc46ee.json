{"ast":null,"code":"import axios from 'axios';\nimport PieChartVue from './DiskPieChart.vue';\nexport default {\n  name: 'MachineStatus',\n  components: {\n    PieChartVue\n  },\n  data() {\n    return {\n      systemInfo: {},\n      loading: true,\n      error: ''\n    };\n  },\n  computed: {\n    chartData() {\n      if (!this.systemInfo.disk_usage || Object.keys(this.systemInfo.disk_usage).length === 0) {\n        return {\n          labels: [],\n          datasets: []\n        };\n      }\n      const labels = Object.keys(this.systemInfo.disk_usage);\n      const usedSpace = Object.values(this.systemInfo.disk_usage).map(disk => disk.used || 0);\n      const freeSpace = Object.values(this.systemInfo.disk_usage).map(disk => disk.free || 0);\n      return {\n        labels: ['Espace Utilisé', 'Espace Libre'],\n        datasets: [{\n          label: 'Utilisation des Disques (Go)',\n          backgroundColor: ['#FF6384', '#36A2EB'],\n          data: [usedSpace.reduce((acc, val) => acc + val, 0), freeSpace.reduce((acc, val) => acc + val, 0)]\n        }]\n      };\n    }\n  },\n  mounted() {\n    this.fetchMachineStatus();\n  },\n  methods: {\n    async fetchMachineStatus() {\n      try {\n        const response = await axios.get('http://localhost:8000/api/machine/status/');\n        this.systemInfo = response.data;\n      } catch (err) {\n        this.error = 'Erreur lors de la récupération des données.';\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","PieChartVue","name","components","data","systemInfo","loading","error","computed","chartData","disk_usage","Object","keys","length","labels","datasets","usedSpace","values","map","disk","used","freeSpace","free","label","backgroundColor","reduce","acc","val","mounted","fetchMachineStatus","methods","response","get","err"],"sources":["C:\\Users\\GUERBER\\Desktop\\SERVDECON\\PROJET\\FRONTEND\\frontend-vvcraft2\\src\\components\\Serveurs\\MachineStatus.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h3>Utilisation des Disques</h3>\r\n    <PieChartVue v-if=\"!loading && systemInfo.disk_usage\" :data=\"chartData\" />\r\n    <p v-else>Chargement des données...</p>\r\n    <p v-if=\"error\">{{ error }}</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport PieChartVue from './DiskPieChart.vue';\r\n\r\nexport default {\r\n  name: 'MachineStatus',\r\n  components: {\r\n    PieChartVue,\r\n  },\r\n  data() {\r\n    return {\r\n      systemInfo: {},\r\n      loading: true,\r\n      error: '',\r\n    };\r\n  },\r\n  computed: {\r\n    chartData() {\r\n      if (!this.systemInfo.disk_usage || Object.keys(this.systemInfo.disk_usage).length === 0) {\r\n        return { labels: [], datasets: [] };\r\n      }\r\n\r\n      const labels = Object.keys(this.systemInfo.disk_usage);\r\n      const usedSpace = Object.values(this.systemInfo.disk_usage).map(disk => disk.used || 0);\r\n      const freeSpace = Object.values(this.systemInfo.disk_usage).map(disk => disk.free || 0);\r\n\r\n      return {\r\n        labels: ['Espace Utilisé', 'Espace Libre'],\r\n        datasets: [\r\n          {\r\n            label: 'Utilisation des Disques (Go)',\r\n            backgroundColor: ['#FF6384', '#36A2EB'],\r\n            data: [\r\n              usedSpace.reduce((acc, val) => acc + val, 0), \r\n              freeSpace.reduce((acc, val) => acc + val, 0)\r\n            ]\r\n          }\r\n        ]\r\n      };\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchMachineStatus();\r\n  },\r\n  methods: {\r\n    async fetchMachineStatus() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8000/api/machine/status/');\r\n        this.systemInfo = response.data;\r\n      } catch (err) {\r\n        this.error = 'Erreur lors de la récupération des données.';\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.machine-status {\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n  background-color: #f9f9f9;\r\n}\r\nh3 {\r\n  color: #555;\r\n}\r\ncanvas {\r\n  max-width: 500px;\r\n  margin: 20px auto;\r\n}\r\n</style>\r\n"],"mappings":"AAUA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,WAAU,MAAO,oBAAoB;AAE5C,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,CAAC,CAAC;MACdC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,SAASA,CAAA,EAAG;MACV,IAAI,CAAC,IAAI,CAACJ,UAAU,CAACK,UAAS,IAAKC,MAAM,CAACC,IAAI,CAAC,IAAI,CAACP,UAAU,CAACK,UAAU,CAAC,CAACG,MAAK,KAAM,CAAC,EAAE;QACvF,OAAO;UAAEC,MAAM,EAAE,EAAE;UAAEC,QAAQ,EAAE;QAAG,CAAC;MACrC;MAEA,MAAMD,MAAK,GAAIH,MAAM,CAACC,IAAI,CAAC,IAAI,CAACP,UAAU,CAACK,UAAU,CAAC;MACtD,MAAMM,SAAQ,GAAIL,MAAM,CAACM,MAAM,CAAC,IAAI,CAACZ,UAAU,CAACK,UAAU,CAAC,CAACQ,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACC,IAAG,IAAK,CAAC,CAAC;MACvF,MAAMC,SAAQ,GAAIV,MAAM,CAACM,MAAM,CAAC,IAAI,CAACZ,UAAU,CAACK,UAAU,CAAC,CAACQ,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACG,IAAG,IAAK,CAAC,CAAC;MAEvF,OAAO;QACLR,MAAM,EAAE,CAAC,gBAAgB,EAAE,cAAc,CAAC;QAC1CC,QAAQ,EAAE,CACR;UACEQ,KAAK,EAAE,8BAA8B;UACrCC,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,CAAC;UACvCpB,IAAI,EAAE,CACJY,SAAS,CAACS,MAAM,CAAC,CAACC,GAAG,EAAEC,GAAG,KAAKD,GAAE,GAAIC,GAAG,EAAE,CAAC,CAAC,EAC5CN,SAAS,CAACI,MAAM,CAAC,CAACC,GAAG,EAAEC,GAAG,KAAKD,GAAE,GAAIC,GAAG,EAAE,CAAC;QAE/C;MAEJ,CAAC;IACH;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,kBAAkB,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,kBAAkBA,CAAA,EAAG;MACzB,IAAI;QACF,MAAME,QAAO,GAAI,MAAM/B,KAAK,CAACgC,GAAG,CAAC,2CAA2C,CAAC;QAC7E,IAAI,CAAC3B,UAAS,GAAI0B,QAAQ,CAAC3B,IAAI;MACjC,EAAE,OAAO6B,GAAG,EAAE;QACZ,IAAI,CAAC1B,KAAI,GAAI,6CAA6C;MAC5D,UAAU;QACR,IAAI,CAACD,OAAM,GAAI,KAAK;MACtB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}